<div class="textHead">
  <div class="login_page">
    <h1>Login</h1>

    <%= form_tag login_path do %>
      <p>
        <%= label_tag :email %><br>
        <%= email_field_tag :email %>
      </p>

      <p>
        <%= label_tag :password %><br>
        <%= password_field_tag :password %>
      </p>

      <%= submit_tag "Login" %>
    <% end %>
  </div>
</div>
